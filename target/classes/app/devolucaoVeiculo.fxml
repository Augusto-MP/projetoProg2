<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.DevolucaoVeiculoController">
   <children>
      <MenuBar VBox.vgrow="NEVER">
         <menus>
            <Menu mnemonicParsing="false" onAction="#voltarAoMenu" text="Menu">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#voltarAoMenu" text="Menu" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="267.0" layoutY="55.0" text="Devolução de Veículo">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="204.0" layoutY="107.0" text="Motorista" />
            <Label layoutX="205.0" layoutY="156.0" prefHeight="18.0" prefWidth="44.0" text="Veículo" />
            <Button layoutX="282.0" layoutY="276.0" mnemonicParsing="false" onAction="#voltarAoMenu" text="Cancelar" />
            <Button layoutX="357.0" layoutY="276.0" mnemonicParsing="false" onAction="#devolucaoVeiculo" prefHeight="26.0" prefWidth="62.0" text="Gravar" />
            <TextField fx:id="campoMotorista" editable="false" layoutX="279.0" layoutY="103.0" />
            <ComboBox fx:id="comboVeiculo" layoutX="279.0" layoutY="153.0" onAction="#atualizaCampos" prefWidth="150.0" />
         </children>
      </AnchorPane>
   </children>
</VBox>
